{
    "completion": {
        "enable": true,
        "autoRequire": true,
        "autoRequireFunction": "require",
        "autoRequireNamingConvention": "keep",
        "callSnippet": true,
        "postfix": "@"
    },
    "diagnostics": {
        "disable": [],
        "enable": true,
        "globals": [],
        "globalsRegex": [],
        "severity": {},
        "enables": [],
        "diagnosticInterval": 500
    },
    "signature": {
        "detailSignatureHelper": true
    },
    "hint": {
        "enable": true,
        "paramHint": true,
        "indexHint": true,
        "localHint": true,
        "overrideHint": true
    },
    "runtime": {
        "version": "Lua5.1",
        "requireLikeFunction": [],
        "frameworkVersions": [],
        "extensions": [],
        "requirePattern": []
    },
    "workspace": {
        "ignoreDir": [],
        "ignoreGlobs": [],
        "library": [],
        "workspaceRoots": [],
        "preloadFileSize": 99999999,
        "encoding": "utf-8",
        "moduleMap": [],
        "reindexDuration": 50000,
        "enableReindex": false
    },
    "resource": {
        "paths": []
    },
    "codeLens": {
        "enable": true
    },
    "strict": {
        "requirePath": false,
        "typeCall": false,
        "arrayIndex": true
    },
    "semanticTokens": {
        "enable": true
    },
    "references": {
        "enable": true,
        "fuzzySearch": true,
        "shortStringSearch": false
    },
    "hover": {
        "enable": true
    },
    "documentColor": {
        "enable": true
    }
}